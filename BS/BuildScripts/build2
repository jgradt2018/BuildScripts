    		</tstamp> 
    		<echo> timeh= ${file.timeh}</echo>
    		<echo> username= ${sf.usernameCSSDev} </echo>
    		<echo> sf.serverurl}=${sf.serverurl}  deployRoot=${pathx} </echo>
       	    <sf:deploy username="${sf.usernameCSSDev}" password="${sf.password}" serverurl="${sf.serverurl}" deployRoot="${pathx}" checkOnly="true"  logType="Debugonly" pollWaitMillis="250000" />
