	<target name="retrieveEfnoldev" depends="init">
			<mkdir dir="${pathx}"/>
		    <tstamp>
					<format property="file.timex" pattern="yyyyMMdd_HHmmss" locale="en"/>
			</tstamp> 
					<echo> timeh = ${file.timeh}</echo>
	 		<!-- Retrieve the contents into another directory -->
		    <sf:retrieve username="${sf.usernameEfnoldev}" password="${sf.password}" serverurl="${sf.serverurl}" retrieveTarget="${pathx}" unpackaged="${baseDir}\BuildScripts\package.xml" />	
			<tstamp>
				<format property="file.timeh2" pattern="yyyyMMdd_HHmmss" locale="en"/>
			</tstamp> 
					<echo> timeh2 = ${file.timeh2}</echo>
	</target>
		
